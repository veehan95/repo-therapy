{
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "eslint.enable": true,
  "eslint.packageManager": "npm",
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.eol": "\n",
  "files.exclude": {
    "bin": true,
    ".DS_Store": true,
    ".fleet": true,
    ".gitignore": true,
    ".husky": true,
    ".idea": true,
    ".npmignore": true,
    ".vscode": true,
    "yarn.lock": true,
    "*.log": true,
    "eslint.config.mjs": true,
    "logs": true,
    "tsconfig.json": true,
    "node_modules": true
  },
  "files.watcherExclude": {
    ".git": true,
    ".vscode": true,
    ".husky": true,
    "yarn.lock": true,
    "package-lock.json": true,
    ".DS_Store": true,
    ".DS_Store?": true,
    "._*": true,
    ".Spotlight-V100": true,
    ".Trashes": true,
    "ehthumbs.db": true,
    "Thumbs.db": true,
    ".idea": true,
    "*.swp": true,
    "*.swo": true,
    "*~": true,
    ".workspace": true,
    "tmp": true,
    "temp": true,
    "*.tmp": true,
    "*.temp": true,
    "*.tgz": true,
    "*.tar.gz": true,
    "*.zip": true,
    "*.rar": true,
    "*.7z": true,
    "node_modules": true,
    ".npm": true,
    ".yarn": true,
    ".yarn/cache": true,
    ".yarn/unplugged": true,
    ".yarn/build-state.yml": true,
    ".yarn/install-state.gz": true,
    ".pnp.*": true,
    ".env": true,
    ".env.local": true,
    ".env.*.local": true,
    ".husky/_": true,
    "logs": true,
    "*.log": true,
    "npm-debug.log*": true,
    "yarn-debug.log*": true,
    "yarn-error.log*": true,
    "lerna-debug.log*": true,
    "coverage": true,
    ".nyc_output": true,
    "*.lcov": true,
    ".jest": true,
    "vitest-report.html": true,
    ".cache": true,
    ".parcel-cache": true,
    ".eslintcache": true,
    ".stylelintcache": true,
    ".rpt2_cache": true,
    ".rts2_cache_cjs": true,
    ".rts2_cache_es": true,
    ".rts2_cache_umd": true,
    "dist": true,
    "bin": true,
    "build": true,
    "out": true,
    "*.tsbuildinfo": true,
    "*.js.map": true,
    "*.d.ts.map": true,
    ".env.development.local": true,
    ".env.test.local": true,
    ".env.production.local": true,
    ".knexpresso": true,
    "uploads": true,
    "credentials": true,
    "docker-compose.yml": true,
    ".dist": true
  }
}